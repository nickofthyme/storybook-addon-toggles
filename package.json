{
  "name": "storybook-addon-toggles",
  "version": "0.0.6",
  "description": "Storybook addon support global toggles",
  "keywords": [
    "storybook-addons",
    "toggles"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nickofthyme/storybook-addon-toggles"
  },
  "type": "module",
  "author": "Nick Partridge <nick.ryan.partridge@gmail.com>",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./preview": {
      "types": "./dist/index.d.ts",
      "import": "./dist/preview.js",
      "require": "./dist/preview.cjs"
    },
    "./preset": "./dist/preset.cjs",
    "./manager": "./dist/manager.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "yarn build --watch",
    "start": "run-p build:watch \"storybook --quiet\"",
    "prerelease": "zx scripts/prepublish-checks.js",
    "release": "yarn build && auto shipit",
    "release:dry": "yarn build && auto shipit --dry-run",
    "storybook": "storybook dev -p 6006",
    "storybook:dev": "onchange -i -k './src/**/*' -- yarn storybook --ci",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@storybook/icons": "1.4.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.0.0",
    "@storybook/react-vite": "^9.0.0",
    "@types/node": "^22",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "auto": "^11.1.1",
    "boxen": "^7.1.1",
    "npm-run-all2": "^6.2.3",
    "onchange": "^7.1.0",
    "prettier": "^3.3.3",
    "prompts": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^9.0.0",
    "ts-dedent": "^2.2.0",
    "tsup": "^8.2.4",
    "typescript": "^5.8.2",
    "vite": "^5.3.5",
    "zx": "^8.4.1"
  },
  "peerDependencies": {
    "storybook": "^9.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "exportEntries": [
      "src/index.ts"
    ],
    "managerEntries": [
      "src/manager.tsx"
    ],
    "previewEntries": [
      "src/preview.ts"
    ],
    "nodeEntries": [
      "src/preset.ts"
    ]
  },
  "storybook": {
    "displayName": "Addon Background Toggle",
    "supportedFrameworks": [
      "react"
    ]
  }
}
